  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.21.0",
    "eslint-config-next": "^15.2.0",
    "eslint-config-prettier": "^10.0.2",
    "prettier": "^3.5.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "engines": {
    "node": "22.x",
    "pnpm": ">=10.5.2",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
